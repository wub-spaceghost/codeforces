#include <stdio.h>
#include <iostream>
#include <conio.h>
#include <string.h>

using namespace std;

int main(){
    int k, i,len,a;
    string n;
    cin>>n;
    cin>>k;
    
    len=n.size();
    for(i=1;i<k;i++){
        i=i+0;
        
        if(n[len-1]=='0'){
            a=stoi(n);
            a=a/10;
        }
        else if(n[len-1!='0']){
            a=stoi(n);
            a=a-1;
        }
    }
    cout<<a<<endl;
    return 0;
}
